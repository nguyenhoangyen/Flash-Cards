<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<p><%= alert %></p>

<%= form_tag sessions_path do %>
	<p>
		<%= label_tag :email %>
		<%= text_field_tag :email %>
	</p>
	<p>
		<%= label_tag :password %>
		<%= password_field_tag :password %>
	</p>
	<p>
		<%= submit_tag "Log in" %>
	</p>
<% end %>